generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id      String    @id @default(auto()) @map("_id") @db.ObjectId
  name    String
  email   String 
  phone   String
  pan     String
  amount  String
}
model Admin {
 id        String    @id @default(auto()) @map("_id") @db.ObjectId
 email     String    @unique
 password   String
 isAdmin   Boolean? @default(false)
}